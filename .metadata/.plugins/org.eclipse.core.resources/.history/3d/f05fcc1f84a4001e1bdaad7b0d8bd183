package 변수및자료형;

public class Ex01입출력 {

	public static void main(String[] args) {
		// 주석 : 코드에 영향을 끼치지 않는 메모
		// 단축키
		// 1) Ctrl+ : 글자크기 늘리기
		// 2) Ctrl- : 글자크기 늘리기
		// 3) Ctrl + D : 한 줄 삭제
		// 4) Ctrl + F11 : 실행 단축키
		
		
		// 1. 출력문
		// ---> () 안쪽에 출력하고 싶은 구문 작성
		System.out.println("Hello World!!!!");
		// 출력문 생성 단축키 : syso -> Ctrl + space
		System.out.println("오인영!!!");
		
		// 2. 입력문
		// 2-1) 입력받는 도구 꺼내오기
		//
		Scanner sc = new Scanner(System.in)
		
		
		
		
		
	}

}
